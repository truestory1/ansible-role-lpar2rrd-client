{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    "docker:pinDigests",
    "helpers:pinGitHubActionDigests"
  ],
  "regexManagers": [
{
    "fileMatch": ["(^|/)*.yml$"],
    "matchStrings": ["(?<lookupName>(?:(?<registry>(?:[a-z0-9-]+\\.)+[a-z0-9]+(?::[0-9]{1,6})?)/)?(?<depName>[^:@\\s'\"]*))(?::(?<currentValue>[^@\\s]+))?(?:@(?<currentDigest>sha256:[a-f0-9]+))?\\s*#\\s*renovate:\\s+datasource=(?<datasource>docker)"]
}
]

}
